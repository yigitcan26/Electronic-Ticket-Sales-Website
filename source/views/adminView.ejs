                              <form method="POST" action="/admin/addVehicleToRoute">
                                  <div class="row-form style-3" style="justify-content:left; gap:40px;">
                                    <div class="inner-row-form style-2">
                                      <div class="seclect-box" style="padding-top: 0px;" >
                                          <h4 class="title-create-item">Select Vehicle</h4>
                                          <div id="item-create" class="dropdown" style="z-index: auto;">
                                              <select id="vehicleID" name="vehicleID" 
                                                  style="
                                                      border: 1px solid rgba(138,138,160,0.3);
                                                      outline: 0;
                                                      -webkit-box-shadow: none;
                                                      -moz-box-shadow: none;
                                                      box-shadow: none;
                                                      font-size: 18px;
                                                      border-radius: 8px;
                                                      padding: 13px 15px;
                                                      width: 100%;
                                                      background: transparent;
                                                      color: #8A8AA0;">
                                                <% 
                                                  const uniqueVehicleIDs = Array.from(new Set(vehicles.map(vehicle => vehicle.vehicleID)));
                                                  uniqueVehicleIDs.forEach(uniqueVehicleID => { 
                                                    const vehicleInfo = vehicles.find(vehicle => vehicle.vehicleID === uniqueVehicleID);
                                                    const combinedInfo = `${uniqueVehicleID} - ${vehicleInfo.vehicleType}`;
                                                %>
                                                  <option value="<%= uniqueVehicleID %>"><%= combinedInfo %></option>
                                                <% }); %>   
                                              </select>
                                          </div>
                                      </div>
                                  </div>
                                    <div class="inner-row-form style-2">
                                        <div class="seclect-box" style="padding-top: 0px;" >
                                            <h4 class="title-create-item">Select Route</h4>
                                            <div id="item-create" class="dropdown" style="z-index: auto;">
                                                <select id="routeID" name="routeID" 
                                                    style="
                                                        border: 1px solid rgba(138,138,160,0.3);
                                                        outline: 0;
                                                        -webkit-box-shadow: none;
                                                        -moz-box-shadow: none;
                                                        box-shadow: none;
                                                        font-size: 18px;
                                                        border-radius: 8px;
                                                        padding: 13px 15px;
                                                        width: 100%;
                                                        background: transparent;
                                                        color: #8A8AA0;">
                                                  <% 
                                                    // Tüm rotaların stationOrder ve place bilgilerini içeren bir nesne oluştur
                                                    const routeInfo = {};
                                                    routes.forEach(route => {
                                                      const { routeID, stationOrder, place } = route;
                                                      if (!routeInfo[routeID]) {
                                                        routeInfo[routeID] = { minStationOrder: Infinity, maxStationOrder: -Infinity, places: [] };
                                                      }
                                                      const info = routeInfo[routeID];
                                                      info.minStationOrder = Math.min(info.minStationOrder, stationOrder);
                                                      info.maxStationOrder = Math.max(info.maxStationOrder, stationOrder);
                                                      info.places.push(place);
                                                    });
                                                  
                                                    // Her bir uniqueRoutesID3 için ilgili bilgileri yazdır
                                                    const uniqueRoutesIDs2 = Array.from(new Set(routes.map(routesID => routesID.routeID)));
                                                    uniqueRoutesIDs2.forEach(uniqueRoutesID3 => { 
                                                      const info = routeInfo[uniqueRoutesID3];
                                                      const firstPlace = info.places[0];
                                                      const lastPlace = info.places[info.places.length - 1];
                                                  %>
                                                  <option value="<%= uniqueRoutesID3 %>"><%= `${uniqueRoutesID3} - ${firstPlace} - ${lastPlace}` %></option>
                                                  <% }); %>              
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                  </div>
                                  <button style="margin-top:20px;" class="sc-button style fl-button pri-3" type="submit">Add Vehicle to Route</button>
