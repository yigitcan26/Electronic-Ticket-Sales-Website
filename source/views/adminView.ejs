                                    function prinmap() {
                                      var denemeseat = "";
                                      var why = document.getElementById('seats');
                                      var mmm = [buttonClickData.map(entry => [entry.row, entry.column])];
                                      for(var i = 0; i < mmm[0].length ; i++)
                                      {
                                        denemeseat += "[" + mmm[0][i] + "] ";
                            
                                      }

                                      
                                      why.innerHTML = denemeseat;
                                     
                                    }
                                    //olmadı input gibi yapıp formdan yararlan
                                    function submitData() {
