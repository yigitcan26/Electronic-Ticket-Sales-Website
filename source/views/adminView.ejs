                      <% vehicles.forEach(allVehicles => { %>
                        <p style="padding: 10px; border-bottom: 1px solid gray;">Vehicle Type - <%= allVehicles.vehicleType %> <br> Vehicle ID - <%= allVehicles.vehicleID %> <br> Route ID - <%= allVehicles.routeID %>
                        </p>
                      <% }) %>
                      </div>
                  </div>                              
              </div>
          </div>
      </div>
  </div>
</section>


